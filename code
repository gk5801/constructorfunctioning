package primitiveDT;
class car
{
	String name;
	String color;
	int model;
	car()
	{
		System.out.println("I'm a default contructor");
	}
	car(String n,String c,int m)
    {
	    name=n;
	    color=c;
	    model=m;
		System.out.println( name + "  " + color + " " + model);
    }
	car(String name,String color)
	{
		this.name=name;
		this.color=color;
		System.out.println(this.name + "  " + this.color + " " );
	}

}
public class constructor
{

	public static void main(String[] args)
	{
		// TODO Auto-generated method stub
		car c1 = new car("audi","black",4839);
		car c2 = new car("mercedees","silver");
		car c3 = new car();
	}

}
